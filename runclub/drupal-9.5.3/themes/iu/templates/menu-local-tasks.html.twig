{{ attach_library('iu/local-tasks') }}
{#
/**
 * @file
 * Override menu local tasks to provide custom styles.
 */
#}
{% if primary %}
  <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
  <ul class="local-tasks primary">{{ primary }}</ul>
{% endif %}
{% if secondary %}
  <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
  <ul class="local-tasks secondary">{{ secondary }}</ul>
{% endif %}
